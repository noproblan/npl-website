{
    "name": "npl/website",
    "description": "The npL website",
    "type": "project",
    "license": "AGPLv3",
    "autoload": {
        "psr-4": {
            "Npl\\Apps\\Website\\Backend\\": "apps/website/backend/src",
            "Npl\\Accounting\\": "src/Accounting",
            "Npl\\Common\\": "src/Common",
            "Npl\\Editorial\\": "src/Editorial",
            "Npl\\LanOrganisation\\": "src/LanOrganisation",
            "Npl\\Marketing\\": "src/Marketing",
            "Npl\\Shared\\": "src/Shared",
            "Npl\\IAM\\": "src/IAM"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "authors": [
        {
            "name": "Fabian \"Shadow\" Schweizer",
            "email": "fa.schweizer@gmail.com"
        }
    ],
    "config": {
        "platform": {
            "php": "8.2"
        },
        "allow-plugins": {
            "symfony/runtime": true
        }
    },
    "require": {
        "sprain/swiss-qr-bill": "^4.6",
        "fpdf/fpdf": "^1.84",
        "ramsey/uuid": "^4.5",
        "egulias/email-validator": "^3.2",
        "lambdish/phunctional": "^2.1",
        "ext-pdo": "*",
        "aura/sqlquery": "4.x-dev",
        "symfony/dependency-injection": "^6.2",
        "symfony/http-kernel": "^6.2",
        "symfony/framework-bundle": "^6.2",
        "symfony/dotenv": "^6.2",
        "symfony/config": "^6.2",
        "symfony/symfony": "^6.2",
        "monolog/monolog": "^3.3",
        "symfony/console": "^6.2"
    },
    "require-dev": {
        "composer/composer": "^2.5",
        "phpunit/phpunit": "^9.6",
        "squizlabs/php_codesniffer": "^3.7",
        "phpmd/phpmd": "^2.13",
        "vimeo/psalm": "^5.13.1",
        "psalm/plugin-phpunit": "^0.18.4",
        "php-standard-library/psalm-plugin": "^2.2.1",
        "dg/bypass-finals": "^1.4",
        "psalm/plugin-symfony": "^v5.0.3"
    }
}
